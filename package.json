{
  "name": "hltransslater",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "dev:web": "expo start --web",
    "build:web": "expo export --platform web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "security-check": "node scripts/security-check.js",
    "style-check": "node scripts/style-check.js",
    "pre-commit": "npm run lint && npm run type-check && npm run security-check && npm run style-check",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "npx expo start --clear",
    "prepare": "husky install",
    "check-modules": "node scripts/check-native-modules.js",
    "build": "node scripts/build-app.js",
    "build:ios": "node scripts/build-app.js ios development",
    "build:android": "node scripts/build-app.js android development",
    "build:ios:prod": "node scripts/build-app.js ios production",
    "build:android:prod": "node scripts/build-app.js android production",
    "build:local": "node scripts/build-app.js ios development --local",
    "setup:dev-build": "node scripts/setup-dev-build.js",
    "start:dev": "expo start --dev-client",
    "start:dev:android": "expo start --dev-client --android",
    "start:dev:ios": "expo start --dev-client --ios",
    "build:dev:android": "expo run:android",
    "build:dev:ios": "expo run:ios",
    "prebuild": "expo prebuild --clean",
    "prebuild:check": "expo prebuild --check",
    "verify:netinfo": "node scripts/verify-netinfo.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-voice/voice": "^3.1.5",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@reduxjs/toolkit": "^2.9.0",
    "@shopify/react-native-skia": "2.2.12",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "docx": "^9.5.1",
    "eslint-config-expo": "10.0.0",
    "expo": "54.0.12",
    "expo-application": "^7.0.7",
    "expo-audio": "~1.0.13",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "6.0.13",
    "expo-device": "8.0.9",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "^17.0.7",
    "expo-media-library": "~18.2.0",
    "expo-print": "~15.0.7",
    "expo-router": "6.0.10",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "15.0.8",
    "i18next": "^25.5.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "lucide-react-native": "^0.475.0",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-text-ticker": "^1.15.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "react-native-view-shot": "^4.0.3",
    "react-native-vision-camera": "^4.7.2",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "react-native-zip-archive": "^6.1.2",
    "react-redux": "^9.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^20.0.2",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@react-native-ml-kit/text-recognition",
          "react-native-zip-archive"
        ]
      }
    }
  }
}
